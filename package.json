{
  "name": "domoja-ui",
  "version": "0.0.1",
  "author": "Bruno Chabrier",
  "homepage": "https://github.com/bchabrier/domoja",
  "private": true,
  "scripts": {
    "start": "ionic serve --address 192.168.0.10 --no-open",
    "clean": "ionic-app-scripts clean",
    "build": "ionic build",
    "lint": "ionic-app-scripts lint",
    "ionic:serve:before": "bin/update_project",
    "ionic:build:before": "bin/update_project",
    "ionic:build": "ionic-app-scripts build --prod --wwwDir ~/domoja/www",
    "ionic:build:after": "sed -e 's!<title>Domoja.*</title>!<title>Domoja</title>!' < src/login.html > ~/domoja/www/login.html; cp src/manifest-auth.json ~/domoja/www; cd ~/domoja/www/assets/favicon && rm -f *.zip readme.txt"
  },
  "dependencies": {
    "@angular/animations": "5.2.11",
    "@angular/common": "5.2.11",
    "@angular/compiler": "5.2.11",
    "@angular/compiler-cli": "5.2.11",
    "@angular/core": "5.2.11",
    "@angular/forms": "5.2.11",
    "@angular/http": "5.2.11",
    "@angular/platform-browser": "5.2.11",
    "@angular/platform-browser-dynamic": "5.2.11",
    "@ionic-native/core": "~4.18.0",
    "@ionic-native/splash-screen": "~4.18.0",
    "@ionic-native/status-bar": "~4.18.0",
    "@ionic/storage": "2.2.0",
    "@types/socket.io-client": "^1.4.32",
    "@types/web-animations-js": "2.2.0",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "rxjs": "5.5.11",
    "socket.io-client": "^2.1.1",
    "sw-toolbox": "3.6.0",
    "web-animations-js": "2.1.3",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.2.1",
    "typescript": "2.6.2"
  },
  "description": "An Ionic project"
}
